@model IEnumerable<MarketWeight.Core.Usuario>

<h2>Lista de Usuarios</h2>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
            <th>Email</th>
            <th>Saldo</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
    @foreach (var usuario in Model)
    {
        <tr>
            <td>@usuario.IdUsuario</td>
            <td>@usuario.Nombre @usuario.Apellido</td>
            <td>@usuario.Email</td>
            <td>@usuario.Saldo</td>

        </tr>
    }
    </tbody>
</table>
